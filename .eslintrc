{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "plugins": [
    "flowtype"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-param-reassign": "off",
    "import/prefer-default-export": "off",
    "no-unused-expressions": "off",
    "no-prototype-builtins": "off",
    "padded-blocks": "off"
  }
}
